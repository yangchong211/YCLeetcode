        </activity>
        <activity android:name=".activity.FirstActivity"/>
        <activity android:name=".activity.SecondActivity"/>
        <activity android:name=".livebus.ThirdActivity"/>
        <activity android:name=".livebus.ThirdActivity1"/>
        <activity android:name=".livebus.ThirdActivity2"/>
        <activity android:name=".livebus.ThirdActivity3"/>
        <activity android:name=".livebus.ThirdActivity4"/>
        <activity android:name=".activity.FourActivity"/>
    </application>

</manifest>